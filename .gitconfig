[user]
	name = yoshihiko.shiraki
	email = shirakiya.pv@gmail.com
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[core]
	editor = vim
[color]
    status = auto
    diff = auto
    branch = auto
    interactive = auto
    grep = auto
[alias]
    st = status
    sh = show
    co = checkout
    ad = add
    cm = commit
    br = branch
    ft = fetch
    rb = rebase
    ps = push
    lg = log
    df = diff
    gr = grep
